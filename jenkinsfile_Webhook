pipeline {
   agent any
    stages {
         stage('pre -build') {
            steps {
                sh 'echo Pre-build'
                sh 'python3 --version'
            }
        }
        stage('build') {
            steps {
                
                echo 'build in progress'
                sh 'mkdir pawpaw'
                sh 'dir'
            }
        }
    }
}
